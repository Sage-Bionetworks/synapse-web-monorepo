{
  "name": "markdown-it-synapse",
  "version": "1.1.17",
  "description": "tag for markdown-it markdown parser.",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs",
    "types": "./dist/index.d.ts"
  },
  "keywords": [
    "markdown-it-plugin",
    "markdown-it",
    "markdown",
    "markdownitSynapse",
    "synapse"
  ],
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "type-check": "tsc --build",
    "build": "tsup",
    "lint": "eslint src/*",
    "test": "vitest",
    "coverage": "vitest --coverage",
    "prepublishOnly": "pnpm install && pnpm nx run markdown-it-synapse:build"
  },
  "homepage": "https://github.com/Sage-Bionetworks/synapse-web-monorepo/tree/main/packages/markdown-it-synapse",
  "repository": {
    "type": "git",
    "url": "git://github.com/Sage-Bionetworks/synapse-web-monorepo.git"
  },
  "bugs": {
    "url": "https://github.com/Sage-Bionetworks/synapse-web-monorepo/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/markdown-it": "^13.0.9",
    "@types/node": "^20.17.41",
    "@vitest/coverage-v8": "^3.2.2",
    "@vitest/ui": "^3.2.4",
    "markdown-it": "^14.1.0",
    "markdown-it-testgen": "~0.1.6",
    "tslib": "^2.8.1",
    "tsup": "^8.4.0",
    "typescript": "5.8.3",
    "vite-config": "workspace:*",
    "vitest": "^3.2.2"
  },
  "directories": {
    "test": "test"
  },
  "author": "Jay"
}
