    "@types/node": "^20.17.41",
    "@vitest/coverage-v8": "^3.2.2",
    "@vitest/ui": "^3.2.4",
    "markdown-it": "^14.1.0",
    "markdown-it-testgen": "~0.1.6",
    "tslib": "^2.8.1",
    "typescript": "5.8.3",
    "vite": "^6.3.5",
