{
  "name": "markdown-it-synapse-table",
  "version": "1.0.8",
  "description": "markdown-it-plugin for table formatting in Synapse.org",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "markdown-it-plugin",
    "markdown-it",
    "markdown",
    "synapse",
    "table"
  ],
  "scripts": {
    "clean": "rimraf ./dist && rimraf *.tsbuildinfo",
    "build": "vite build",
    "lint": "eslint src/*",
    "test": "vitest",
    "coverage": "vitest --coverage",
    "prepublishOnly": "pnpm install && pnpm nx run markdown-it-synapse-table:build"
  },
  "homepage": "https://github.com/Sage-Bionetworks/synapse-web-monorepo/packages/markdown-it-synapse-table",
  "repository": {
    "type": "git",
    "url": "git://github.com/Sage-Bionetworks/synapse-web-monorepo.git"
  },
  "bugs": {
    "url": "https://github.com/Sage-Bionetworks/synapse-web-monorepo/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/markdown-it": "catalog:",
    "@types/node": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "@vitest/ui": "catalog:",
    "markdown-it": "^14.1.0",
    "markdown-it-testgen": "~0.1.6",
    "tslib": "^2.6.2",
    "typescript": "5.8.3",
    "vite": "catalog:",
    "vite-config": "workspace:*",
    "vitest": "catalog:"
  },
  "author": "Jay"
}
