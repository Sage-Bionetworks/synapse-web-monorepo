  "scripts": {
    "clean": "rimraf dist storybook-static coverage",
    "prepublishOnly": "pnpm install && pnpm nx run synapse-react-client:build",
    "start-js": "vite",
    "start": "storybook dev -p 6060",
    "start-deprecated-demo": "start-js",
    "test": "jest",
    "test:ci": "jest --watchAll=false --coverage",
    "test:coverage": "jest --coverage",
    "build": "rimraf dist && pnpm --parallel run \"/^build:.*/\"",
    "build:copy-assets": "copyfiles -u 1 \"src/**/*.{css,scss,svg}\" dist",
    "build:js": "vite build",
    "build:umd:dev": "vite build -c vite.config.umd.ts --mode development",
    "build:umd:prod": "vite build -c vite.config.umd.ts --mode production",
    "storybook:build": "pnpm storybook build",
    "type-check": "tsc --noEmit",
    "link-src": "pnpm link && cd ./node_modules/react && pnpm link && cd ../react-router && pnpm link && cd ../react-router-dom && pnpm link && cd ../../",
    "unlink-src": "pnpm unlink && cd ./node_modules/react && pnpm unlink && cd ../react-router && pnpm unlink && cd ../react-router-dom && pnpm unlink && cd ../../",
    "lint": "eslint src",
    "watch:js": "tsc --watch"
  },
  "nx": {
    "targets": {
      "storybook:build": {
        "dependsOn": [
          "^build"
        ]
      }
    }
  },
  "browser": {
    "timers": "timers-browserify"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 2 chrome version",
      "last 2 firefox version",
      "last 2 safari version",
      "last 2 ie version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,yml,scss,css,md}": "prettier --write"
  },
  "msw": {
    "workerDirectory": "public"
  }
