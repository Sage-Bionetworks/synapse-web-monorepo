    "react-test-renderer": "^19.1.0",
    "storybook": "^8.6.12",
    "typescript": "5.8.3",
    "vite": "^6.3.6",
    "vite-config": "workspace:*",
    "vitest": "^3.2.2",
    "vitest-when": "^0.6.0"
