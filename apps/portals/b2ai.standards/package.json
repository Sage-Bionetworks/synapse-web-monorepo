{
  "name": "b2ai.standards",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@mui/icons-material": "^7.1.1",
    "@mui/material": "^7.1.1",
    "@sage-bionetworks/synapse-portal-framework": "workspace:*",
    "@sage-bionetworks/synapse-types": "workspace:*",
    "katex": "^0.16.22",
    "lodash-es": "^4.17.21",
    "pluralize": "^8.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.5.3",
    "synapse-react-client": "workspace:*",
    "vite": "^7.1.2"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "sass": "^1.87.0",
    "tslib": "^2.8.1",
    "typescript": "5.8.3",
    "vite-config": "workspace:*",
    "vitest": "^3.2.2"
  },
  "scripts": {
    "start": "vite",
    "build": "pnpm save-build-date && vite build && pnpm run generate-sitemap b2ai.standards",
    "preview": "vite preview",
    "type-check": "tsc --build",
    "lint": "eslint src",
    "_generate-sitemap": "node ../../synapse-portal-framework/sitemap/generate-sitemap.cjs \\\"$VITE_PORTAL_KEY\\\"",
    "generate-sitemap": "dotenv -- pnpm _generate-sitemap",
    "save-build-date": "mkdir -p public && date > public/build-date.txt"
  }
}
