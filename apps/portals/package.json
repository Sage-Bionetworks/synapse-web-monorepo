{
  "name": "portals",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.13",
    "@mui/material": "^5.15.13",
    "@react-hookz/web": "^23.1.0",
    "@sage-bionetworks/react-base-table": "^1.13.4",
    "@sage-bionetworks/synapse-types": "workspace:*",
    "@tanstack/react-query": "5.22.2",
    "clone-deep": "^4.0.1",
    "dayjs": "^1.11.10",
    "katex": "^0.16.10",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "plotly.js-basic-dist": "^2.30.0",
    "react": "^18.2.0",
    "react-bootstrap": "^1.6.8",
    "react-cookie": "4.0.0",
    "react-dom": "^18.2.0",
    "react-plotly.js": "^2.6.0",
    "react-router-dom": "^5.3.4",
    "react-spinners": "^0.13.8",
    "react-switch": "^7.0.0",
    "sass": "^1.71.1",
    "synapse-react-client": "workspace:*",
    "@upsetjs/react": "^1.11.0"
  },
  "scripts": {
    "clean": "rimraf coverage",
    "lint": "eslint src",
    "copy-assets": "cp src/config/style/favicon* public/; cp src/config/style/logo.svg public/; cp src/config/style/socialmedia.png public/",
    "save-build-date": "date > public/build-date.txt",
    "start": "vite",
    "build": "pnpm copy-assets; pnpm save-build-date; vite build",
    "copy-test-configuration": "cp -r src/test-configuration/ src/config/",
    "test": "pnpm run copy-test-configuration && vitest",
    "test:ci": "pnpm run copy-test-configuration && vitest run --coverage",
    "generate-sitemap": "node sitemap/generate-sitemap.cjs",
    "type-check": "tsc --noEmit",
    "portal:build": "export $(grep -v '^#' .env | xargs); chmod +x ./build.sh; ./build.sh ${PORTAL}",
    "portal:start": "pnpm run start >/dev/null 2>&1 &",
    "portal:stop": "pid=$(lsof -i :3000 -t) && kill ${pid}",
    "e2e": "playwright test",
    "e2e:chromium": "playwright test --project=chromium",
    "e2e:report": "pnpm exec playwright show-report",
    "e2e:report:blob": "pnpm exec playwright merge-reports --reporter html ./blob-report && pnpm e2e:report",
    "playwright:update": "pnpm add -D @playwright/test@latest; pnpm playwright install"
  },
  "devDependencies": {
    "@playwright/test": "^1.42.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/katex": "^0.5.0",
    "@types/lodash": "^4.17.0",
    "@types/node": "^18.19.24",
    "@types/plotly.js": "^2.29.2",
    "@types/react": "18.2.64",
    "@types/react-dom": "^18.2.22",
    "@types/react-plotly.js": "^2.6.3",
    "@types/react-router-dom": "^5.3.3",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.7",
    "@vitest/utils": "^0.34.7",
    "assert": "^2.1.0",
    "buffer": "^6.0.3",
    "dotenv": "^16.4.5",
    "https-browserify": "^1.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^21.1.2",
    "memfs": "^3.5.3",
    "path-browserify": "^1.0.1",
    "pluralize": "^8.0.0",
    "postcss-normalize": "^10.0.1",
    "process": "^0.11.10",
    "source-map-explorer": "^2.5.3",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "timers-browserify": "^2.0.12",
    "tslib": "^2.6.2",
    "typescript": "5.4.2",
    "url": "^0.11.3",
    "util": "^0.12.5",
    "vite": "^5.2.10",
    "vite-config": "workspace:*",
    "vitest": "^0.34.6",
    "whatwg-fetch": "^3.6.20"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "last 2 versions",
      "Firefox ESR",
      "not ie <= 10",
      "not ie_mob <= 10",
      "not bb <= 10",
      "not op_mob <= 12.1"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
