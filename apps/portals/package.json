{
  "name": "portals",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.12",
    "@mui/styles": "^5.10.10",
    "@sage-bionetworks/react-base-table": "^1.13.2",
    "clone-deep": "^4.0.1",
    "dayjs": "^1.11.6",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "plotly.js-basic-dist": "^2.18.0",
    "react": "^18.2.0",
    "react-app-polyfill": "^1.0.1",
    "react-bootstrap": "^1.5.2",
    "react-cookie": "4.0.0",
    "react-dom": "^18.2.0",
    "react-plotly.js": "^2.5.1",
    "react-query": "3.39.1",
    "react-router-dom": "^5.3.3",
    "react-spinners": "^0.13.8",
    "react-switch": "^7.0.0",
    "react-use": "^17.3.1",
    "sass": "^1.30.0",
    "synapse-react-client": "^3.1.21"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(synapse-react-client|lodash-es|jest*)/)"
    ]
  },
  "scripts": {
    "copy-assets": "cp src/config/style/favicon* public/; cp src/config/style/logo.svg public/; cp src/config/style/socialmedia.png public/",
    "save-build-date": "date > public/build-date.txt",
    "start": "craco start",
    "build": "pnpm copy-assets; pnpm save-build-date; CI=false GENERATE_SOURCEMAP=false craco --max_old_space_size=7168 build",
    "copy-test-configuration": "cp -r src/test-configuration/ src/config/",
    "test": "pnpm run copy-test-configuration && craco test --env=jsdom --setupFiles ./src/tests/setupTests.ts --watchAll=false",
    "generate-sitemap": "node sitemap/generate-sitemap.js",
    "eject": "craco eject",
    "analyze": "pnpm run copy-test-configuration && craco --max_old_space_size=4096 build && source-map-explorer 'build/static/js/*.js'",
    "link-src": "pnpm link synapse-react-client && pnpm link react && pnpm link react-router && pnpm link react-router-dom",
    "unlink-src": "pnpm unlink synapse-react-client && pnpm unlink react && pnpm unlink react-router && pnpm unlink react-router-dom && pnpm install --check-files"
  },
  "devDependencies": {
    "@craco/craco": "^7.0.0",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^27.5.0",
    "@types/katex": "^0.5.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "11.10.4",
    "@types/plotly.js": "^1.54.22",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-plotly.js": "^2.2.4",
    "@types/react-router-dom": "^5.3.3",
    "assert": "^2.0.0",
    "babel-jest": "^27.5.1",
    "buffer": "^6.0.3",
    "https-browserify": "^1.0.0",
    "identity-obj-proxy": "^3.0.0",
    "katex": "^0.10.0-rc.1",
    "jest-environment-jsdom": "^27.5.1",
    "markdown-it": "^13.0.1",
    "markdown-it-br": "^1.0.0",
    "markdown-it-center-text": "^1.0.4",
    "markdown-it-container": "^2.0.0",
    "markdown-it-emphasis-alt": "^1.0.8",
    "markdown-it-inline-comments": "^1.0.1",
    "markdown-it-strikethrough-alt": "^1.0.0",
    "markdown-it-sub-alt": "^1.0.0",
    "markdown-it-sup-alt": "^1.0.2",
    "markdown-it-synapse": "^1.1.10",
    "markdown-it-synapse-heading": "^1.0.1",
    "markdown-it-synapse-math": "^3.0.4",
    "markdown-it-synapse-table": "^1.0.6",
    "memfs": "^3.4.7",
    "path-browserify": "^1.0.1",
    "pluralize": "^8.0.0",
    "postcss-normalize": "^10.0.1",
    "process": "^0.11.10",
    "raf": "^3.4.1",
    "react-scripts": "^5.0.1",
    "source-map-explorer": "^2.5.2",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "timers-browserify": "^2.0.12",
    "typescript": "4.9.5",
    "url": "^0.11.0",
    "util": "^0.12.4",
    "whatwg-fetch": "^3.6.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "last 2 versions",
      "Firefox ESR",
      "not ie <= 10",
      "not ie_mob <= 10",
      "not bb <= 10",
      "not op_mob <= 12.1"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "babel-jest": "^27.5.1",
    "jest-environment-jsdom": "^27.5.1",
    "goober": "2.1.9",
    "js-yaml": "3.13.1",
    "react": "18.2.0",
    "react-hot-toast": "2.2.0",
    "@types/plotly.js": "^1.54.10",
    "@types/react": "^18.0.17",
    "minimatch": "^3.1.2"
  }
}
